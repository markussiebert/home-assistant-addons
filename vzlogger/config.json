{
  "name": "vzLogger",
  "version": "3a41d77aa80cf6c83763b9785180f0cad729ba2a",
  "slug": "vzLogger",
  "description": "vzlogger for homeassistant",
  "arch": ["armhf", "armv7", "amd64"],
