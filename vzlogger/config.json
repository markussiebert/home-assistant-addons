{
  "name": "vzLogger",
  "version": "7308760123f28ed2d4663eca9eb6f79476f549c1",
  "slug": "vzLogger",
  "description": "vzlogger for homeassistant",
  "arch": ["armhf", "armv7", "amd64"],
  "startup": "application",
  "boot": "auto",
  "kernel_modules": true,
  "usb": true,
  "udev": true,
  "gpio": true,
  "apparmor": false,
  "privileged": ["SYS_RAWIO"],
  "devices": ["/dev/mem"],
  "options": {
    "retry": 0,
    "verbosity": 0,
    "log": ""
  },
  "schema": {
    "log_level": "list(trace|debug|info|notice|warning|error|fatal)?",
    "device": "device(subsystem=tty)",
    "retry": "int",
    "verbosity": "int",
    "log": "str"
  },
  "ports": {
    "8088/tcp": 10080
  },
  "ports_description": {
    "8080/tcp": "vzlogger server"
  },
  "image": "ghcr.io/markussiebert/homeassistant-addon-vzlogger/{arch}"
}
