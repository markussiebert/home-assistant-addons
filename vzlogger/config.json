{
  "name": "vzLogger",
  "version": "0998398e262504506aaadb2e3940415eb11d1b6e",
  "slug": "vzLogger",
  "description": "vzlogger for homeassistant",
  "arch": ["armhf", "armv7", "amd64"],
  "map": ["share"],
  "startup": "application",
  "boot": "auto",
  "kernel_modules": true,
  "usb": true,
  "udev": true,
  "gpio": true,
  "apparmor": false,
  "privileged": ["SYS_RAWIO"],
  "devices": ["/dev/mem"],
  "options": {
    "configfile": "/share/vzlogger.conf"
